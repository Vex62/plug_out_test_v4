<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_main_page">

    <fragment
        android:id="@+id/navigation_main_page"
        android:name="com.example.plug_out_test_v4.ui.main_page.MainPage"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_main_page" />

    <fragment
        android:id="@+id/navigation_select_apps"
        android:name="com.example.plug_out_test_v4.ui.select_apps.SelectAppsFragment"
        android:label="@string/title_select_apps"
        tools:layout="@layout/fragment_select_apps" />

    <fragment
        android:id="@+id/navigation_overlay"
        android:name="com.example.plug_out_test_v4.ui.overlay.OverlayFragment"
        android:label="@string/title_overlay"
        tools:layout="@layout/fragment_overlay" />
</navigation>